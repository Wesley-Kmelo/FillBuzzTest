        int c =1;
        String n ;
       
        for(int i = 1; i <= 100 ; i ++){           
                if ((c % 3 == 0) && ( c % 5 == 0)) {
                    n = "FIZZBUZZ";
                }else if (c % 3 == 0) {
                    n="FIZZ";
                }else if (c % 5 == 0){
                    n="BUZZ";
                } else {
                    n = String.valueOf(c); 
                }
            System.out.println(n);
            c++; 
